{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": ".",
      "entryFile": "main",
      "sourceRoot": "src"
    }
  }
}
